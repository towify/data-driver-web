{
  "name": "data-driver-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck",
    "build": "ng build --configuration=production",
    "pre-build:main-china": "node build.cli.mjs main china",
    "pre-build:test-china": "node build.cli.mjs test china",
    "pre-build:main-oversea": "node build.cli.mjs main oversea",
    "pre-build:test-oversea": "node build.cli.mjs test oversea",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "ng e2e",
    "prepare": "husky install",
    "lint": "eslint --ext .js, .jsx, .ts, .tsx src",
    "pre-commit": "lint-staged",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.html": "prettier --write --parser angular",
    "*.component.html": "prettier --write --parser angular",
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,scss}": "prettier --write"
